#include <iostream>
using namespace std;

int mayorNumero(int a, int b, int c){
	int mayor = a;
	
	if(b > mayor) {
		mayor = b; 
	}
	else if (c > mayor)
	{
		mayor = c;
	}
	return mayor;
}

int main() {
	int num1, num2, num3;
	cout<<"Ingrese tres numeros: ";
	cin>>num1>>num2>>num3;
	
	int mayor = mayorNumero(num1, num2, num3);
	
	cout<<"El mayor de los tres numeros es: "<<mayor<<endl;
	
	return 0;
}
