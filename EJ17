#include <iostream>
using namespace std;

int factorial(int n) {
    int resultado = 1;
    
    for (int i = 2; i <= n; i++) {
        resultado *= i;
    }
    return result;
}

int main() {
    int num;
    cout<<"Ingresa un numero: ";
    cin>>num;
